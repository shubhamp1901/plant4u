{
    "name": "astro-chat-backend",
    "version": "1.0.0",
    "description": "Backend for Astro Chat",
    "main": "index.js",
    "private": true,
    "scripts": {
        "seed": "node ./tests/seed/seed.js",
        "test:watch": "jest --coverage --config ./jest.config.js --notify --watchAll",
        "test:local": "jest --coverage --config ./jest.config.js --notify",
        "test:ci": "jest --coverage --config ./jest.config.js --forceExit",
        "client": "react-scripts start",
        "dev": "nodemon server.js",
        "concurrent": "concurrently \"nodemon server.js\" \"npm run client\"",
        "start": "node server.js",
        "serve:proxy": "ngrok http 3000"
    },
    "author": "Lu-Vuong <lu-vuongle@hotmail.com>",
    "license": "MIT",
    "dependencies": {
        "@sendgrid/mail": "^7.7.0",
        "agora-access-token": "^2.0.4",
        "aws-sdk": "^2.1273.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "compression": "^1.7.4",
        "cors": "^2.8.4",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "express-sslify": "^1.2.0",
        "express-validator": "^5.3.0",
        "googleapis": "^110.0.0",
        "gravatar": "^1.8.0",
        "helmet": "^3.15.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.3.1",
        "multer": "^1.4.5-lts.1",
        "passport": "^0.4.0",
        "passport-facebook": "^2.1.1",
        "passport-google-oauth": "^1.0.0",
        "passport-jwt": "^4.0.0",
        "slugify": "^1.3.4",
        "socket.io": "^2.2.0",
        "url-parse": "^1.5.10",
        "vue": "^2.6.10",
        "vue-server-renderer": "^2.6.10"
    },
    "devDependencies": {
        "concurrently": "^7.6.0",
        "eslint": "^5.9.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-prettier": "^3.0.0",
        "jest": "^23.6.0",
        "morgan": "^1.9.1",
        "nodemon": "^1.18.9",
        "prettier": "^1.15.3",
        "supertest": "^3.3.0",
        "winston": "^3.1.0"
    }
}
